drop udp $HONEYNET any -> $EXTERNAL_NET 7 (msg:"SCAN cybercop udp bomb"; content:"cybercop"; reference:arachnids,363; classtype:bad-unknown; sid:636; rev:1;)
drop udp $HONEYNET any -> $EXTERNAL_NET any (msg:"SCAN Webtrends Scanner UDP Probe"; content: "|0A|help|0A|quite|0A|"; reference:arachnids,308; classtype:attempted-recon; sid:637; rev:2;)
drop tcp $HONEYNET any -> $EXTERNAL_NET 22 (msg:"SCAN SSH Version map attempt"; flow:to_server,established; content:"Version_Mapper"; nocase;  classtype:network-scan; sid:1638; rev:4;)
